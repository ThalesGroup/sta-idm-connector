<resource xmlns="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
    xmlns:c="http://midpoint.evolveum.com/xml/ns/public/common/common-3"
    xmlns:icfs="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/resource-schema-3"
    xmlns:icfc="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/connector-schema-3"
    xmlns:icfcsta="http://midpoint.evolveum.com/xml/ns/public/connector/icf-1/bundle/com.evolveum.polygon.STA-IdM-connector/com.connid.sta.connector.STARestConnector"
    xmlns:org="http://midpoint.evolveum.com/xml/ns/public/common/org-3"
    xmlns:q="http://prism.evolveum.com/xml/ns/public/query-3"
    xmlns:ri="http://midpoint.evolveum.com/xml/ns/public/resource/instance-3"
    xmlns:t="http://prism.evolveum.com/xml/ns/public/types-3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    oid="33333333-0000-0000-0000-000000000000">
    <name>SafeNet Trusted Access</name>
    <connectorRef type="c:ConnectorType">
        <filter>
            <q:equal>
                <q:path>c:connectorType</q:path>
                <q:value>com.connid.sta.connector.STARestConnector</q:value>
            </q:equal>
        </filter>
    </connectorRef>
    <connectorConfiguration>
        <icfc:configurationProperties>
            <icfcsta:pageSize>30</icfcsta:pageSize>
            <icfcsta:apiKey>
                <clearValue>apikey</clearValue>
            </icfcsta:apiKey>
            <icfcsta:APIBaseURL>https://api.{your_STA_zone}.safenetid.com/api/v1/tenants/{your_STA_tenant-code}/</icfcsta:APIBaseURL>
        </icfc:configurationProperties>
    </connectorConfiguration>
    <schemaHandling>
        <objectType>
            <kind>account</kind>
            <intent>default</intent>
            <displayName>Account</displayName>
            <default>true</default>
            <objectClass>ri:AccountObjectClass</objectClass>
            <attribute>
                <ref>ri:email</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>emailAddress</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:firstName</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>givenName</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:lastName</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>familyName</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:userName</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>name</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:address</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>locality</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:city</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/city</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:mobileNumber</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>telephoneNumber</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:postalCode</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/postalCode</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:state</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/state</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:country</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/country</path>
                    </source>
                </outbound>
            </attribute>
			<attribute>
                <ref>icfs:name</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>name</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:userPrincipalName</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>extension/userPrincipalName</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:isSynchronized</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <expression>
                        <value>false</value>
                    </expression>
                </outbound>
            </attribute>
            <association>
                <ref>ri:STA Group</ref>
                <displayName>STA Group Membership</displayName>
                <tolerant>false</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <kind>entitlement</kind>
                <intent>group</intent>
                <direction>objectToSubject</direction>
                <associationAttribute>ri:members</associationAttribute>
                <valueAttribute>ri:id</valueAttribute>
                <explicitReferentialIntegrity>false</explicitReferentialIntegrity>
            </association>
        </objectType>
        <objectType>
            <kind>entitlement</kind>
            <intent>group</intent>
            <displayName>Group</displayName>
            <default>true</default>
            <objectClass>ri:GroupObjectClass</objectClass>
            <attribute>
                <ref>ri:description</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>description</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:name</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>name</path>
                    </source>
                </outbound>
            </attribute>
			<attribute>
                <ref>icfs:name</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <source>
                        <path>name</path>
                    </source>
                </outbound>
            </attribute>
            <attribute>
                <ref>ri:isSynchronized</ref>
                <tolerant>true</tolerant>
                <exclusiveStrong>false</exclusiveStrong>
                <outbound>
                    <authoritative>true</authoritative>
                    <exclusive>false</exclusive>
                    <strength>normal</strength>
                    <expression>
                        <value>false</value>
                    </expression>
                </outbound>
            </attribute>
        </objectType>
    </schemaHandling>

    <synchronization>
        <objectSynchronization>
            <name>Account</name>
            <objectClass>AccountObjectClass</objectClass>
            <kind>account</kind>
            <intent>default</intent>
            <focusType>c:UserType</focusType>
            <enabled>true</enabled>
            <correlation>
                <q:equal>
                    <q:path>c:name</q:path>
                    <expression>
                        <path>$shadow/attributes/name</path>
                    </expression>
                </q:equal>
                <q:description>User correlation</q:description>
            </correlation>
            <reconcile>false</reconcile>
            <reaction>
                <situation>linked</situation>
                <synchronize>true</synchronize>
            </reaction>
            <reaction>
                <situation>deleted</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#deleteFocus</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>unlinked</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#link</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>unmatched</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#addFocus</handlerUri>
                </action>
            </reaction>
        </objectSynchronization>
        <objectSynchronization>
            <name>Group</name>
            <objectClass>ri:GroupObjectClass</objectClass>
            <kind>entitlement</kind>
            <intent>group</intent>
            <focusType>c:RoleType</focusType>
            <enabled>true</enabled>
            <correlation>
                <q:equal xmlns="">
                    <q:path>c:name</q:path>
                    <expression>
                        <path>$shadow/attributes/name</path>
                    </expression>
                </q:equal>
                <q:description>Group correlation</q:description>
            </correlation>
            <reconcile>false</reconcile>
            <reaction>
                <situation>deleted</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#deleteFocus</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>unlinked</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#link</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>unmatched</situation>
                <synchronize>true</synchronize>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#addFocus</handlerUri>
                </action>
            </reaction>
            <reaction>
                <situation>linked</situation>
                <synchronize>true</synchronize>
                <reconcile>false</reconcile>
                <action>
                    <handlerUri>http://midpoint.evolveum.com/xml/ns/public/model/action-3#sync</handlerUri>
                </action>
            </reaction>
        </objectSynchronization>
    </synchronization>
  
</resource>